<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ServiciosActivity">

    <Button
        android:id="@+id/btnAmbar"
        android:layout_width="338dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_button"
        android:backgroundTint="#f7959a"
        android:text="SPA AMBAR"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="229dp" />

    <Button
        android:id="@+id/btnLimpieza"
        android:layout_width="329dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_button"
        android:backgroundTint="#f7959a"
        android:text="LIMPIEZA FACIAL PROFUNDA"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="634dp" />

    <Button
        android:id="@+id/btnKids"
        android:layout_width="328dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_button"
        android:backgroundTint="#f7959a"
        android:text="SPA KIDS"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="507dp" />

    <Button
        android:id="@+id/btnDiamante"
        android:layout_width="344dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_button"
        android:backgroundTint="#f7959a"
        android:text="SPA DIAMANTE"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="305dp" />

    <Button
        android:id="@+id/btnRubi"
        android:layout_width="339dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_text"
        android:backgroundTint="#f7959a"
        android:text="SPA RUBI"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="160dp" />

    <Button
        android:id="@+id/btnDepilacion"
        android:layout_width="335dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_button"
        android:backgroundTint="#f7959a"
        android:text="DEPILACION EN CERA CORPORAL"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="574dp" />

    <Button
        android:id="@+id/btnTurquesa"
        android:layout_width="328dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_button"
        android:backgroundTint="#f7959a"
        android:text="SPA TURQUESA"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="372dp" />

    <Button
        android:id="@+id/btnLove"
        android:layout_width="331dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_button"
        android:backgroundTint="#f7959a"
        android:text="LOVE SPA"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="440dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="141dp"
        android:layout_height="45dp"
        android:fontFamily="casual"
        android:text="Servicios"
        android:textColor="@color/black"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="99dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:background="#f7959a"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteY="43dp" />


</androidx.constraintlayout.widget.ConstraintLayout>